<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<digester-rules>
  <rule pattern="*/roleNameChild" registerMethod="setChild"/>
  <rule pattern="*/roleNameDeprecatedChild" registerMethod="setDeprecatedChild"/>
  <rule pattern="*/roleNameInheritedChilds" registerMethod="registerInheritedChilds"/>
  <rule pattern="*/roleNameInheritedChildDocOnDerived" registerMethod="setInheritedChildDocOnDerived"/>
  <rule pattern="*/roleNameChildNoDoc" registerMethod="setChildNoDoc"/>
  <rule pattern="*/roleNameInheritedChildDocWithDescriptionOverride" registerMethod="setInheritedChildDocWithDescriptionOverride"/>
  <rule pattern="*/roleNameInheritedWithoutOverride" registerMethod="setInheritedWithoutOverride"/>
  <rule pattern="*/roleNameInheritedChildNonSelected" registerMethod="setInheritedChildNonSelected"/>
  <rule pattern="*/roleNameChildOverriddenOnlyParentAnnotated" registerMethod="setChildOverriddenOnlyParentAnnotated"/>
  <rule pattern="*/roleNameText" registerTextMethod="registerText" />
  <rule pattern="*/roleNameIgnored" registerTextMethod="setNotConfigChildButAttribute" />
</digester-rules>