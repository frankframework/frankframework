<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:jms="http://www.tibco.com/namespaces/ws/2004/soap/binding/JMS" xmlns:jndi="http://www.tibco.com/namespaces/ws/2004/soap/apis/jndi" xmlns:tns="http://nn.nl/WSDL/AccountInformation/AccountInformation/1/GetPaymentReferenceAndPayLink/1" xmlns:ns1="http://nn.nl/XSD/AccountInformation/AccountInformation/1/GetPaymentReferenceAndPayLink/1" xmlns:ns2="http://nn.nl/XSD/AccountInformation/DomainValues" xmlns:ns3="http://nn.nl/XSD/Generic/MessageHeader/2" targetNamespace="http://nn.nl/WSDL/AccountInformation/AccountInformation/1/GetPaymentReferenceAndPayLink/1">
	<wsdl:documentation>IGNORE</wsdl:documentation>
	<wsdl:types>
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cmh="http://nn.nl/XSD/Generic/MessageHeader/2" xmlns:mtp="http://nn.nl/XSD/AccountInformation/DomainValues" xmlns="http://nn.nl/XSD/AccountInformation/AccountInformation/1/GetPaymentReferenceAndPayLink/1" targetNamespace="http://nn.nl/XSD/AccountInformation/AccountInformation/1/GetPaymentReferenceAndPayLink/1" attributeFormDefault="unqualified" elementFormDefault="qualified">
			<xs:import namespace="http://nn.nl/XSD/Generic/MessageHeader/2"/>
			<xs:import namespace="http://nn.nl/XSD/AccountInformation/DomainValues"/>
	
	
	<xs:element name="GetPaymentReferenceAndPayLink_Request">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Debtor">
					<xs:annotation>
						<xs:documentation>Debtor contains the debtor information required by the MailToPay API including restrictions defined by aforementioned API</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LastName" type="xs:string"/>
							<xs:element name="FirstName" type="xs:string" minOccurs="0"/>
							<xs:element name="Partner" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Contract">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PayableItems" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DocumentNumber">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="12"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RepetitionItem">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="3"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="DocumentItemNumber">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="04"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SubItem">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="03"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="TotalAmount" type="mtp:AmountType"/>
				<xs:element name="Invoice">
					<xs:annotation>
						<xs:documentation>Invoice is based on the invoice fields required by the MailToPay API including restrictions defined by aforementioned API</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Description" type="mtp:InvoiceDescriptionType"/>
							<xs:element name="Date" type="mtp:InvoiceDateType"/>
							<xs:element name="DueDate" type="mtp:InvoiceDueDateType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>DueDate has a cosmetic function and is not used to determine experation date</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PaymentData">
					<xs:annotation>
						<xs:documentation>PaymentData holds fields required by the MailToPay API to process the GetPaylink request</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="BatchId" type="mtp:BatchIdType"/>
							<xs:element name="Concerning" type="mtp:ConcerningType"/>
							<xs:element name="ExpirationDate" type="mtp:ExpirationDateType"/>
							<xs:element name="ReturnURL" type="mtp:ReturnURLType">
								<xs:annotation>
									<xs:documentation>ReturnURL is the URL to which the customer should be send after completing the transaction</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetPaymentReferenceAndPayLink_Response">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PaymentDetails" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PaymentId" type="mtp:PaymentIdType"/>
							<xs:element name="PaymentURL" type="mtp:PaymentURLType"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="cmh:Result"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
		<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://nn.nl/XSD/AccountInformation/DomainValues" targetNamespace="http://nn.nl/XSD/AccountInformation/DomainValues" attributeFormDefault="unqualified" elementFormDefault="qualified">
	<xs:simpleType name="InvoiceNumberType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InvoiceDateType">
		<xs:restriction base="DateType"/>
	</xs:simpleType>
	<xs:simpleType name="InvoiceDescriptionType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InvoiceAmountType">
		<xs:restriction base="AmountType"/>
	</xs:simpleType>
	<xs:simpleType name="InvoiceDueDateType">
		<xs:restriction base="DateType"/>
	</xs:simpleType>
	<xs:simpleType name="InvoiceIBANType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="34"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BatchIdType">
		<xs:annotation>
			<xs:documentation>BatchId is used to group payments together in the MailToPay administrator interface</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ConcerningType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExpirationDateType">
		<xs:annotation>
			<xs:documentation>ExpirationDate is the date after which the Payment URL expires</xs:documentation>
		</xs:annotation>
		<xs:restriction base="DateType"/>
	</xs:simpleType>
	<xs:simpleType name="PaymentReferenceType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReturnURLType">
		<xs:annotation>
			<xs:documentation>ReturnURL is the URL to which the customer should be send after completing the transaction</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatusCodeType">
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:simpleType name="StatusDateType">
		<xs:restriction base="DateType"/>
	</xs:simpleType>
	<xs:simpleType name="StatusDescriptionType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="PaymentIdType">
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:simpleType name="PaymentURLType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="FirstNameType">
		<xs:annotation>
			<xs:documentation>FirstName restriction is based on restrictions in the MailToPay API</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LastNameType">
		<xs:annotation>
			<xs:documentation>LastName restriction is based on restrictions in the MailToPay API</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DateType">
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="AmountType">
		<xs:annotation>
			<xs:documentation>Amount format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="13"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
