		<add key="BankId.Acquirer.AcquirerID" value="1234" />
		<add key="BankId.Acquirer.DirectoryUrl" value="http://example.com/directory" />
		<add key="BankId.Acquirer.TransactionUrl" value="http://example.com/transaction" />
		<add key="BankId.Acquirer.StatusUrl" value="http://example.com/status" />

		<add key="BankId.Acquirer.Certificate.Alias" value="dummy_alias" />
