  </xs:group>
  <xs:attributeGroup name="AbstractSenderDeclaredAttributeGroup">
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>name of the sender</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute ref="active" />
