  <xs:complexType name="MonitorType">
    <xs:annotation>
      <xs:documentation>&lt;p&gt;Example configuration:&lt;/p&gt;
 &lt;pre&gt;&lt;code&gt;&amp;lt;Monitor name=&amp;quot;Receiver Shutdown&amp;quot; destinations=&amp;quot;MONITOR_LOG&amp;quot;&amp;gt;
    &amp;lt;AlarmTrigger severity=&amp;quot;WARNING&amp;quot;&amp;gt;
        &amp;lt;Event&amp;gt;Receiver Shutdown&amp;lt;/Event&amp;gt;
    &amp;lt;/AlarmTrigger&amp;gt;
    &amp;lt;ClearingTrigger severity=&amp;quot;WARNING&amp;quot;&amp;gt;
        &amp;lt;Event&amp;gt;Receiver Shutdown&amp;lt;/Event&amp;gt;
    &amp;lt;/ClearingTrigger&amp;gt;
 &amp;lt;/Monitor&amp;gt;&lt;/code&gt;&lt;/pre&gt;</xs:documentation>
