     &lt;td align="left"&gt;&amp;nbsp;&lt;/th&gt;
     &lt;td align="left"&gt;0-23&lt;/td&gt;
     &lt;td align="left"&gt;&amp;nbsp;&lt;/th&gt;
     &lt;td align="left"&gt;&lt;code&gt;, - * /&lt;/code&gt;&lt;/td&gt;
   &lt;/tr&gt;
   &lt;tr&gt;
     &lt;td align="left"&gt;&lt;code&gt;Day-of-month&lt;/code&gt;&lt;/td&gt;
