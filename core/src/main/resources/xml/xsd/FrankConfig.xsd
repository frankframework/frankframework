              <xs:element name="Locker" type="LockerType" />
              <xs:group ref="ManagerElementGroup" />
              <xs:group ref="MessageLogElementGroup" />
              <xs:group ref="OutputValidatorElementGroup" />
              <xs:group ref="OutputWrapperElementGroup" />
