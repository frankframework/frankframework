  <xs:attributeGroup name="AbstractFileSystemSenderDeclaredAttributeGroup">
    <xs:attribute name="action">
      <xs:annotation>
        <xs:documentation>If parameter [&lt;code&gt;action&lt;/code&gt;] is set, then the attribute action value will be overridden with the value of the parameter.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union memberTypes="FileSystemActionAttributeValuesType variableRef" />
      </xs:simpleType>
