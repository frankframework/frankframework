<module>
	<scheduler>
		<job
			name="cleanup File System"
			cronExpression="${cleanup.filesystem.cron}"
			description="Cleanup the file system"
			function="cleanupFileSystem"
		>
			<directoryCleaner
				directory="${fxf.dir}"
				subdirectories="true"
				notExistWarn="false"
			/>
			<directoryCleaner
				directory="${log.dir}"
				retention="${cleanup.log.dir.retention}"
			/>
		</job>
	</scheduler>
</module>