<module>
	<scheduler>
		<job
			name="Cleanup the database (IbisLock)"
			cronExpression="${cleanup.lock.cron}"
			description="Cleanup the database (IbisStore)"
			function="cleanupLock"
		>
			<locker
				datasourceName="${jdbc.datasource.default}"
				objectId="CleanupLock"
				ignoreTableNotExist="true"
			/>
		</job>
	</scheduler>
</module>