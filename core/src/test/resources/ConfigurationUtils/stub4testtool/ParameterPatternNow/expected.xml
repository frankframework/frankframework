<configuration>
	  <adapter name="ParameterPatternNow">
      <receiver name="testtool-ParameterPatternNow">
         <listener className="nl.nn.adapterframework.receivers.JavaListener"
                   serviceName="testtool-ParameterPatternNow"/>
      </receiver>
		    <receiver name="ParameterPatternNow">
			      <listener className="nl.nn.adapterframework.receivers.JavaListener"
                   name="ParameterPatternNow"/>
		    </receiver>
      <receiver name="testtool-ParameterPatternNow-1">
         <listener className="nl.nn.adapterframework.receivers.JavaListener"
                   serviceName="testtool-ParameterPatternNow-1"/>
      </receiver>

		    <pipeline>
			      <exits>
				        <exit path="READY" state="success"/>
			      </exits>
			      <pipe name="PutParametersInSession"
               className="nl.nn.adapterframework.pipes.PutParametersInSession">
				        <param name="date"
                   type="datetime"
                   pattern="{fixedDate,date,yyyy-MM-dd HH:mm:ss}"/>
				        <forward name="success" path="READY"/>
			      </pipe>
		    </pipeline>
	  </adapter>
</configuration>