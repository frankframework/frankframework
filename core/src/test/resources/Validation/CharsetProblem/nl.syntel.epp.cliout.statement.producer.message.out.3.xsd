					<xs:element name="amountExemptFromVAT" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>amount exempt from VAT in account currency

origin: 
table DOORBELHEADER -&gt; cell DH_PERS_BTWVRIJ

calculate (entity balanceMutation -&gt; amount - VatCharge.amountInAccountCurrency) * cell DH_PERS_BTWVRIJ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="amountVATTaxablePortion" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>amount VAT taxable in account currency

origin:

amount exempt from VAT

origin: 
table DOORBELHEADER -&gt; cell DH_PERS_BTWVRIJ

calculate (entity balanceMutation -&gt; amount - VatCharge.amountInAccountCurrency) * (1 - cell DH_PERS_BTWVRIJ)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="percentageVatTaxable" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>calculate: 
100 -/- (table DOORBELHEADER -&gt; cell DH_PERS_BTWVRIJ)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="calculatedManagementFee" type="calculatedManagementFee" minOccurs="0" maxOccurs="1"></xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
