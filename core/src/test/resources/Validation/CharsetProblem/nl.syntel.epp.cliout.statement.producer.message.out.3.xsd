	<xs:complexType name="balanceDetails">
		<xs:annotation>
			<xs:documentation>specific information for statements about an account</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="currentBalance" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>current balance

origin: 
if no BalanceMutations in message then same as previousBalance
else
table POSITIEMUTATIES_GELD -&gt; cell pmg_saldo_boekdatum 
where account = CashAccount
and MAX(pmg_boekdatum) from BalanceMutations in message
and MAX(pmg_transactienr) from BalanceMutations on MAX(pmg_boekdatum)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currentDate" type="xs:date" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>current date

origin:
equals to Header.dateGenerated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="previousBalance" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>balance on previous statement

origin: 
if exists from StatementHistory with account = CashAccount:
table POSITIEMUTATIES_GELD -&gt; cell pmg_saldo_boekdatum 
where account = CashAccount
and pmg_transactienr = TransactionNumber from StatementHistory with MAX(dateGenerated) and account = CashAccount

if NOT exists from StatementHistory with account = CashAccount:
value = 0 (new account)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="previousDate" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>date of previous statement

origin: 
dateGenerated from StatementHistory 
where MAX(dateGenerated) and account = CashAccount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalCreditTransactions" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>total amount of all credit transactions

origin: 
calculated amount of all credit transactions within daily statement based on de BalanceMutations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalDebitTransactions" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>total amount of all debit transactions

origin: 
calculated amount of all debit transactions within daily statement based on de BalanceMutations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalTransactions" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Total amount of all transactions:
totalCreditTransactions -/- totalDebitTransactions</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
