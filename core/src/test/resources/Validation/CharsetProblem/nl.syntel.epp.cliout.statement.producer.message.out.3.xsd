						<xs:annotation>
							<xs:documentation>Presentatie van het aantal, bijvoorbeeld voor dagafschriften</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="price" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>price of instrument

origin:
table TRANSACTIES -&gt; cell TR_PRIJS_TR_MUNTSRT</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="quantity" type="xs:decimal" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>quantity of the transaction

origin:
IF TransactionType.symbol in (OMWL, OMWS):

WHEN association of SecuritySubTransaction is fundTransaction THEN
	result = table TRANSAKTIES -&gt; cell tr_aantal;

WHEN association of SecuritySubTransaction is referenceFundTransaction THEN
	result = table TRANSAKTIES -&gt; cell tr_aantal * entity ConvertSecurityTransaction -&gt; exchangeRatio;

WHEN entity SecuritySubTransaction.instrument -&gt; decimalsDisplay &lt;&gt; 0 THEN
result = result * entity SecuritySubTransaction.instrument -&gt; priceFactor; 

ELSE IF Transaction.originCode in (PCNL, PCND):

WHEN entity SecuritySubTransaction.instrument -&gt; decimalsDisplay &lt;&gt; 0 THEN 
	result = table TRANSAKTIES -&gt; cell tr_aantal * entity SecuritySubTransaction.instrument -&gt; priceFactor 
ELSE 
	result = table TRANSAKTIES -&gt; cell tr_aantal;

ELSE:

IF entity SecuritySubTransaction.instrument -&gt; decimalsDisplay &lt;&gt; 0 THEN
  table TRANSAKTIES -&gt; cell tr_aantal / (10 ^ entity FundSecuritySubTransaction.instrument-&gt; decimalsDisplay)
ELSE
  table TRANSAKTIES -&gt; cell tr_aantal
	
END IF;

result = ABS(result);</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="bondSubTransaction" type="bondSubTransaction" minOccurs="0" maxOccurs="1"></xs:element>
					<xs:element name="converted" type="convertedSubTransaction" minOccurs="0" maxOccurs="1"></xs:element>
