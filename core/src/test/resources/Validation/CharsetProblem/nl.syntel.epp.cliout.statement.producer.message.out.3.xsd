	<xs:complexType name="convertedSubTransaction">
		<xs:annotation>
			<xs:documentation>Contains the extra fields of funds to be converted within transactions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amount" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>amount to be paid/receive for the converted fund in the transaction currency

origin: 
result = attribute SecuritySubTransaction.quantity * attribute priceToBeSettled;

IF TransactionType.symbol in (OMWL, OMWS):

WHEN association of SecuritySubTransaction is fundTransaction and transaction is type Reversal THEN
	result = -1 * result;

WHEN association of SecuritySubTransaction is referenceFundTransaction and NOT transaction is type Reversal THEN
	result = -1 * result;
	
END IF;

IF Transaction.originCode in (PCNL, PCND):

WHEN table TRANSAKTIES -&gt; cell tr_aantal &lt; 0 THEN
	result = -1 * result;

END IF;</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="amountInAccountCurrency" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>the converted amount in account currency

origin:
amount * exchangeRateTransactionAndAccount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceToBeSettled" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>the price for the converted fund

origin: 

if TransactionType.symbol in (OMWL, OMWS) &amp; association of SecuritySubTransaction is referenceFundTransaction:
table TRANSAKTIES -&gt; cell tr_verrekenbedrag_nieuw_fonds

if TransactionType.symbol in (OMWL, OMWS) &amp; association of SecuritySubTransaction is fundTransaction:
table TRANSAKTIES -&gt; cell tr_verrekenbedrag_oud_fonds

if Transaction.originCode = `PCND¿:
table TRANSAKTIES -&gt; cell tr_verrekenbedrag_nieuw_fonds

if Transaction.originCode = `PCNL¿:
table TRANSAKTIES -&gt; cell tr_verrekenbedrag_oud_fonds</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="currency" minOccurs="1" maxOccurs="1"></xs:element>
		</xs:sequence>
