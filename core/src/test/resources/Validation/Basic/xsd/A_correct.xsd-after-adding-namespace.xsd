<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified"
           targetNamespace="http://www.ing.com/testxmlns" xmlns:a="http://www.ing.com/testxmlns"
           xmlns:b="http://xmlns/b"  xmlns="http://www.ing.com/testxmlns"
    >
  <xs:import namespace="http://xmlns/b" schemaLocation="B.xsd" />
  <xs:element name="Envelope">
    <xs:complexType>
      <xs:sequence>
		  <xs:element name="Body">
		    <xs:complexType>
		      <xs:sequence>
		        <xs:element ref="a:A" />
		      </xs:sequence>
		    </xs:complexType>
		  </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="A">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="b:B" minOccurs="0" maxOccurs="2"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
