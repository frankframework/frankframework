<configuration name="SimpleConfiguration">
	<adapter name="HelloWorld">
		<receiver name="HelloWorld" >
			<listener
					name="HelloWorld"
					className="org.frankframework.receivers.JavaListener"
			/>
		</receiver>
		<pipeline firstPipe="HelloWorld">
			<exit path="EXIT" state="success"/>
			<pipe
					name="HelloWorld"
					className="org.frankframework.pipes.FixedResultPipe"
					returnString="Hello World"
			>
				<forward name="success" path="EXIT"/>
			</pipe>
		</pipeline>
	</adapter>
	<adapter name="HelloOtherWorld">
		<receiver name="HelloOtherWorld">
			<listener name="HelloOtherWorld" className="org.frankframework.receivers.JavaListener"/>
		</receiver>
		<pipeline firstPipe="HelloOtherWorld">
			<exit name="EXIT" state="success"/>
			<pipe name="HelloOtherWorld" returnString="Hello Other World ${digester.property} [${secret}] &lt;@quot;&gt;" className="nl.nn.adapterframework.pipes.FixedResultPipe">
				<forward name="success" path="EXIT"/>
			</pipe>
		</pipeline>
	</adapter>
</configuration>
