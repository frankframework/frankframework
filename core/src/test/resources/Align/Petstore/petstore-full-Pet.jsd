{
    "$schema":"http://json-schema.org/draft-04/schema#",
    "type":"object",
    "additionalProperties":false,
    "properties":{
        "Pet":{
            "$ref":"#/definitions/Pet"
        }
    },
    "definitions":{
        "Pet":{
            "$ref":"#/definitions/petType"
        },
        "Pets":{
            "type":"object",
            "additionalProperties":false,
            "properties":{
                "Pet":{
                    "type":"array",
                    "minItems":1,
                    "items":{
                        "$ref":"#/definitions/Pet"
                    }
                }
            },
            "required":[
                "Pet"
            ]
        },
        "petType":{
            "type":"object",
            "additionalProperties":false,
            "properties":{
                "id":{
                    "type":"integer"
                },
                "name":{
                    "type":"string"
                },
                "tag":{
                    "type":"string"
                }
            },
            "required":[
                "id",
                "name"
            ]
        }
    }
}