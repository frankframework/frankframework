{
    "$schema":"http://json-schema.org/draft-04/schema#",
    "type":"object",
    "additionalProperties":false,
    "properties":{
        "HandleCollectionDisbursementAccount":{
            "$ref":"#/definitions/HandleCollectionDisbursementAccount"
        }
    },
    "definitions":{
        "HandleCollectionDisbursementAccount":{
            "type":"object",
            "additionalProperties":false,
            "properties":{
                "MessageHeader":{
                    "$ref":"#/definitions/MessageHeader"
                },
                "ContractAccount":{
                    "$ref":"#/definitions/ContractAccountType"
                }
            },
            "required":[
                "MessageHeader",
                "ContractAccount"
            ]
        },
        "MessageHeader":{
            "type":"object",
            "additionalProperties":false,
            "properties":{
                "TimestampSend":{
                    "type":"string",
                    "maxLength":20
                },
                "ControlSendDate":{
                    "type":"string",
                    "format":"date"
                }
            },
            "required":[
                "TimestampSend",
                "ControlSendDate"
            ]
        },
        "PolicyAccountType":{
            "type":"object",
            "additionalProperties":false,
            "properties":{
                "insuranceAdministration":{
                    "type":"string",
                    "maxLength":2
                },
                "policyReference":{
                    "type":"string",
                    "maxLength":20
                },
                "bankAccount":{
                    "type":"string",
                    "maxLength":18
                },
                "paymentMethod":{
                    "type":"string",
                    "maxLength":4,
                    "enum":[
                        "10",
                        "20",
                        "30",
                        "40",
                        "50",
                        "60",
                        "70"
                    ]
                },
                "dunningVariant":{
                    "type":"string"
                },
                "mortgageAssignment":{
                    "type":"string",
                    "maxLength":2
                },
                "fiscalAssignment":{
                    "type":"string",
                    "maxLength":2
                },
                "loanAssignment":{
                    "type":"string",
                    "maxLength":1
                },
                "beneficiaryRight":{
                    "type":"string",
                    "maxLength":1
                },
                "outputCode":{
                    "type":"string"
                },
                "portfolioNumber":{
                    "type":"string"
                },
                "sepaNumber":{
                    "type":"string"
                },
                "bic":{
                    "type":"string"
                },
                "startDateSEPAMandate":{
                    "type":"string"
                }
            },
            "required":[
                "insuranceAdministration",
                "policyReference"
            ]
        },
        "ContractAccountType":{
            "type":"object",
            "additionalProperties":false,
            "properties":{
                "accountId":{
                    "type":"string",
                    "maxLength":20
                },
                "startDate":{
                    "type":"string",
                    "format":"date"
                },
                "partyReference":{
                    "type":"string",
                    "maxLength":20
                },
                "partyAdministration":{
                    "type":"string",
                    "maxLength":3
                },
                "currentAccountIndicator":{
                    "type":"string",
                    "maxLength":1
                },
                "contractAccountDescription":{
                    "type":"string",
                    "maxLength":35
                },
                "PolicyAccount":{
                    "type":"array",
                    "items":{
                        "$ref":"#/definitions/PolicyAccountType"
                    }
                }
            },
            "required":[
                "accountId",
                "startDate",
                "currentAccountIndicator",
                "contractAccountDescription",
                "PolicyAccount"
            ]
        }
    }
}