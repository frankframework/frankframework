<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:element name="FindDocuments_Request">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="schemaName" minOccurs="0">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="40"/>
							<xsd:minLength value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="requestUserId">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
							<xsd:maxLength value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="SearchAttributes" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="addressedTo" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="40"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="agreementNumber" minOccurs="0" maxOccurs="unbounded">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="20"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="agreementNumberType" type="agreementNumberType" minOccurs="0" maxOccurs="1"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="FindDocuments_Response">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Paging" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="foundItemCount" type="xsd:integer" minOccurs="0"/>
							<xsd:element name="hasMoreItems" type="xsd:boolean" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SearchResult" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="schemaName" type="xsd:string"/>
							<xsd:element name="internalDocumentId" type="xsd:string"/>
							<xsd:element name="externalDocumentId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="documentTitle" type="xsd:string" minOccurs="0"/>
							<xsd:element name="language" type="xsd:string" minOccurs="0"/>
							<xsd:element name="fileName" minOccurs="0"/>
							<xsd:element name="addressedTo" minOccurs="0"/>
							<xsd:element name="annotation" minOccurs="0"/>
							<xsd:element name="agreementNumber" minOccurs="0"/>
							<xsd:element name="agreementNumberType" minOccurs="0"/>
							<xsd:element name="archiveDate" type="xsd:dateTime" minOccurs="0"/>
							<xsd:element name="authorizedTo" minOccurs="0"/>
							<xsd:element name="batchCode" minOccurs="0"/>
							<xsd:element name="birthDate" type="xsd:date" minOccurs="0"/>
							<xsd:element name="BSN" minOccurs="0"/>
							<xsd:element name="businessUnit" minOccurs="0"/>
							<xsd:element name="claimNumber" minOccurs="0"/>
							<xsd:element name="contractNumber" minOccurs="0"/>
							<xsd:element name="contractNumberHolding" minOccurs="0"/>
							<xsd:element name="descriptionProductGroup" minOccurs="0"/>
							<xsd:element name="documentDetail" minOccurs="0"/>
							<xsd:element name="documentRelationId" minOccurs="0"/>
							<xsd:element name="documentRouting" minOccurs="0"/>
							<xsd:element name="documentType" minOccurs="0"/>
							<xsd:element name="IBAN" minOccurs="0"/>
							<xsd:element name="idDocOriginal" minOccurs="0"/>
							<xsd:element name="issuedDate" type="xsd:date" minOccurs="0"/>
							<xsd:element name="issuer" minOccurs="0"/>
							<xsd:element name="label" minOccurs="0"/>
							<xsd:element name="lastModifiedBy" minOccurs="0"/>
							<xsd:element name="lastModifiedDate" type="xsd:dateTime" minOccurs="0"/>
							<xsd:element name="nameHolding" minOccurs="0"/>
							<xsd:element name="nameInsured" minOccurs="0"/>
							<xsd:element name="nameOrganisation" minOccurs="0"/>
							<xsd:element name="namePolicyHolder" minOccurs="0"/>
							<xsd:element name="organisationId" minOccurs="0"/>
							<xsd:element name="partnerGold" minOccurs="0"/>
							<xsd:element name="partnerIntermediary" minOccurs="0"/>
							<xsd:element name="partnerSilver" minOccurs="0"/>
							<xsd:element name="processId" type="xsd:string" minOccurs="0"/>
							<xsd:element name="productCode" minOccurs="0"/>
							<xsd:element name="quotationNumber" minOccurs="0"/>
							<xsd:element name="referenceDate" type="xsd:date" minOccurs="0"/>
							<xsd:element name="remarkText" minOccurs="0"/>
							<xsd:element name="requestNumber" minOccurs="0"/>
							<xsd:element name="requiredByLawId" minOccurs="0"/>
							<xsd:element name="source" minOccurs="0"/>
							<xsd:element name="sourceTransactionId" minOccurs="0"/>
							<xsd:element name="securityClass" minOccurs="0"/>
							<xsd:element name="subject" minOccurs="0"/>
							<xsd:element name="VTA_ID" minOccurs="0"/>
							<xsd:element name="year" minOccurs="0"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:simpleType name="agreementNumberType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Polis"/>
			<xsd:enumeration value="Lening"/>
			<xsd:enumeration value="Rekening"/>
			<xsd:enumeration value="Deposit"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
