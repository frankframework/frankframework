<email>
	<recipients>
		<recipient name="name" type="cc">***@integrationpartners.nl</recipient>
		<recipient name="name" type="to">***@hotmail.com</recipient>				<!--Required-->
		<recipient name="name" type="to">***@gmail.com</recipient>
	</recipients>
	<from name="name">***@integrationpartners.nl</from>								<!--Required-->
	<replyTo name="name">***@integrationpartners.nl</replyTo>
	<subject>Mail handler check subject</subject>									<!--Required min Length 1 -->
	<message>An email sent by iaf mail handler in result of unit test.</message>	<!--Required-->
	<messageType>text/plain</messageType>											<!-- Required -->
	<messageBase64>false</messageBase64>											<!-- Optional -->
	<attachments>																	<!-- either name or type should contain file extension-->
		<attachment name="test.txt">This is a test file.</attachment>				<!-- url starts with file:/// for local sytem files-->
	</attachments>
 </email>